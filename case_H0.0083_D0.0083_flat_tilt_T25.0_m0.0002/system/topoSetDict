// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    allCells;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (-0.00455 -0.00455 -0.0004) (0.00455 0.00455 0.0087);
    }
    {
        name    internalMesh;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     allCells;
    }
);

