FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      functions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

probesU
{
    type            probes;
    libs            ("libsampling.so");
    writeControl    timeStep;
    writeInterval   5;
    fixedLocations  true;
    fields
    (
        U
        alpha.water
    );
    probeLocations
    (
        (0 0 0.0025)
        (0.0018675 0 0.0025)
        (0 0.0018675 0.0025)
        (-0.0018675 0 0.0025)
        (0 -0.0018675 0.0025)
        (0 0 0.005)
        (0.0018675 0 0.005)
        (0 0.0018675 0.005)
        (-0.0018675 0 0.005)
        (0 -0.0018675 0.005)
        (0 0 0.0075)
        (0.0018675 0 0.0075)
        (0 0.0018675 0.0075)
        (-0.0018675 0 0.0075)
        (0 -0.0018675 0.0075)
    );
}

